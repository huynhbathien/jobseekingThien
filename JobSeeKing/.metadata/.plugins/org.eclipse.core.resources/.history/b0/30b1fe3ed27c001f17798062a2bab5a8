package jobseekingbe.api.controller;

import java.util.ArrayList;
import java.util.List;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestParam;
import org.springframework.web.bind.annotation.RestController;

import jobseekingbe.api.model.JobDTO;


@RestController
public class JobAPI {

	@GetMapping(value = "/api/job")
	public List<JobDTO> getJob(@RequestParam(value = "name", required = false) String name){
		List<JobDTO> jobList=new ArrayList<JobDTO>();
		return jobList;
	}
}
